{
  "name": "Wallet",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created by Arihant and Veerat",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Wallet"
  },
  "permissions": [
    "notifications",
    "activeTab",
    "https://ajax.googleapis.com/",
    "downloads",
    "storage"
  ]
}
